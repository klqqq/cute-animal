<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.java.cute.rest.modules.app.dao.video.AppVideoDao">

    <select id="queryByVideoId" resultType="java.util.HashMap">
        select * from video where video_id = #{value}
    </select>

    <select id="queryList" resultType="java.util.HashMap">
        select * from video
    </select>

</mapper>